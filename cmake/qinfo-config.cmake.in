# A CMake config file for the library, to be used by external projects

@PACKAGE_INIT@

list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")

if(TARGET qinfo::qinfo)
  return()
endif()

include("${CMAKE_CURRENT_LIST_DIR}/CompilerWarnings.cmake")
include("${CMAKE_CURRENT_LIST_DIR}/qinfo-targets.cmake")

if(NOT qinfo_FIND_QUIETLY)
  message(STATUS "Found qinfo version ${qinfo_VERSION}")
endif()
